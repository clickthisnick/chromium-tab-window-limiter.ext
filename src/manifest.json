{
  "name": "TabWindowLimiter",
  "description": "Limit Amount Of Tabs/Windows",
  "version": "0.0.3",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "TabWindowLimiter"
  }
}
